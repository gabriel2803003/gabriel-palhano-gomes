package javaapplication111;
public class JavaApplication111 {

   public static void main(String[] args) {
        int i;
        for( i=0 ; i<=100 ; i++)
{ 
   if (i%3==0 && i%5==0 && i>0)
  { 
   System.out.println("universo");
  }
   else if (i%5==0 && i>0)
  {
    System.out.println("verso");
  }
  else if (i%3==0 )
  {
    System.out.println("uni");
  }
  else
  {
    System.out.println ( i );
  }
}
     
    }
    
}
